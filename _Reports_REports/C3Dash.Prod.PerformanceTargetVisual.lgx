      <Row>
        <Column ColSpan="3">
          <Tabs ID="tabDates" TabLocation="left" TabbingStyle="Static" DefaultTab="Month" ActiveTabBackgroundColor="#f5f5f5" BorderColor="black" InactiveTabBackgroundColor="white">
            <TabPanel Caption="Month" ID="Month">
              <Division ID="divTablesMonth">
                <Rows Class="gdtempbackground" CellSpacing="5">
                  <Row>
                    <Column Class="gdMetricContainer,ThemeAlignTop" RowSpan="2">
                      <Rows>
                        <Row>
                          <Column>
                            <Rows Class="gdMetricContainerEOM">
                              <Row>
                                <Column Class="ThemeAlignCenter,gdMetricContainerHeader">
                                  <Label Caption="EOM GCR" />
                                </Column>
                              </Row>
                              <Row>
                                <Column Class="ThemeAlignTop,ThemeAlignCenter,gdMetricContainerContent">
                                  <DataTable ID="dtEOMGCR" Class="gdMetricTableEOM" Width="200" WidthScale="px" AltRowClass="gdThemeAltRowNoPadding td">
                                    <DataLayer Type="Linked" LinkedDataLayerID="rptMetricsMonth" ID="rptMetricsMonth.Linked">
                                      <ConditionFilter Condition="&quot;@Data.MetricName~&quot; = &quot;Total GCR&quot;" IncludeCondition="True" ID="cfEOMRev" />
                                      <CalculatedColumn Formula="(@Data.MetricVariance~ * @Local.aggTarget~)" ID="calcProjected" />
                                      <CalculatedColumn Formula="(@Data.MetricVariance~ * @Local.aggTarget~) - @Local.aggTarget~" ID="calcVariance" />
                                    </DataLayer>
                                    <DataTableColumn ID="colTarget" Header="Target">
                                      <Label Caption="@Local.aggTarget~" Format="$#,##0" />
                                    </DataTableColumn>
                                    <DataTableColumn ID="colProjected" Header="Projected">
                                      <Label Caption="@Data.calcProjected~" Format="$#,##0" />
                                    </DataTableColumn>
                                    <DataTableColumn ID="colVariance" Header="Variance" Class="noteHighlight">
                                      <Label Caption="@Data.calcVariance~" Format="$#,##0">
                                        <ConditionalClass Condition="@Data.calcVariance~ &lt; 0" Class="ThemeTextNegative" />
                                        <Action Type="Report" ID="actTargetRevenue">
                                          <Target Type="Report" ID="tgtTargetRevenue" RequestForwarding="True" Report="C3Dash.PerformanceTargetRevenue" />
                                          <LinkParams SupID="@Data.SupID~" DepartmentID="@Data.DeptID~" RepID="@Data.RepID~" DisplayName="@Data.DisplayName~" />
                                        </Action>
                                      </Label>
                                    </DataTableColumn>
                                  </DataTable>
                                </Column>
                              </Row>
                            </Rows>
                          </Column>
                        </Row>
                        <Row>
                          <Column>
                            <Rows Class="gdMetricContainer">
                              <Row>
                                <Column Class="ThemeAlignCenter,gdMetricContainerHeader">
                                  <Label Caption="MTD GCR" />
                                </Column>
                              </Row>
                              <Row>
                                <Column Class="ThemeAlignTop,ThemeAlignCenter,gdMetricContainerContent">
                                  <LineBreak />
                                  <Gauge Type="Bar" BarGaugeStyle="VerticalThermometer" ChartHeight="335" ChartWidth="150" ID="gaugeRevenueMonth" LowerBound="0" UpperBound="@Data.MetricTarget~" Value="@Data.MetricValue~" D3Effects="True" BackgroundColor="#DDEBF7" BarColor="#5EC45E" TickCount="8" TickLabelFormat="@Data.MetricFormat~" BarBackgroundColor="white" TickLabelFontSize="8" BarBorderColor="#666666" FontBold="True">
                                    <DataLayer Type="Linked" LinkedDataLayerID="rptMetricsMonth" ID="rptMetricsMonth.Linked0">
                                      <ConditionFilter Condition="&quot;@Data.MetricName~&quot; = &quot;GCR&quot;" IncludeCondition="True" ID="cfSales" />
                                    </DataLayer>
                                    <Action Type="Report" ID="actTargetRevenue">
                                      <Target Type="Report" ID="tgtTargetRevenue" RequestForwarding="True" Report="C3Dash.PerformanceTargetRevenue" />
                                      <LinkParams SupID="@Data.SupID~" DepartmentID="@Data.DeptID~" RepID="@Data.RepID~" DisplayName="@Data.DisplayName~" />
                                    </Action>
                                  </Gauge>
                                  <LineBreak LineCount="2" />
                                  <DataTable ID="dtNewRev" Class="gdMetricTable" Width="200" WidthScale="px">
                                    <DataLayer Type="Linked" LinkedDataLayerID="rptMetricsMonth" ID="rptMetricsMonth.Linked1">
                                      <ConditionFilter Condition="&quot;@Data.MetricName~&quot; = &quot; New GCR&quot; || &quot;@Data.MetricName~&quot; = &quot;Total GCR&quot;" IncludeCondition="True" ID="cfNewRev" />
                                    </DataLayer>
                                    <DataTableColumn ID="colName" Header="GCR">
                                      <Label Caption="@Data.MetricName~" />
                                    </DataTableColumn>
                                    <DataTableColumn ID="colGCR" Header="$">
                                      <Label Caption="@Data.MetricValue~" Format="$#,##0" />
                                    </DataTableColumn>
                                    <DataTableColumn ID="colPercent" Header="%" Class="noteHighlight">
                                      <Label Caption="@Data.MetricVariance~" Format="#,##0.0%" />
                                    </DataTableColumn>
                                  </DataTable>
                                </Column>
                              </Row>
                            </Rows>
                          </Column>
                        </Row>
                      </Rows>
                    </Column>
                    <Column Class="gdMetricContainer,ThemeAlignTop">
                      <Rows>
                        <Row>
                          <Column>
                            <Rows Class="gdMetricContainer">
                              <Row>
                                <Column Class="ThemeAlignCenter,gdMetricContainerHeader">
                                  <Label Caption="AOS" />
                                </Column>
                              </Row>
                              <Row>
                                <Column Class="ThemeAlignTop,ThemeAlignCenter,gdMetricContainerContent">
                                  <Gauge Type="Needle" ChartHeight="222" ID="gaugeAOSMonth" Value="@Data.calcValue~" ChartWidth="263" StartAngle="0" UpperBound="@Data.calcHigher~" TickCount="2" NeedleGaugeEnclosure="None" D3Effects="True" BackgroundColor="#DDEBF7" NeedleThickness="1" GaugeType="HalfTop" LowerBound="@Data.calcLower~" TickLabelFontSize="8" TickLabelRadius="110" MinorTickFrequency="5" TickLabelFormat="@Data.MetricFormat~" CenterOffsetY="-8">
                                    <DataLayer Type="Linked" LinkedDataLayerID="rptMetricsMonth" ID="rptMetricsMonth.Linked2">
                                      <ConditionFilter Condition="&quot;@Data.MetricName~&quot; = &quot;AOS&quot;" IncludeCondition="True" ID="cfAOS" />
                                      <CalculatedColumn ID="calcLower" Formula="@Data.MetricTarget~ * .80" />
                                      <CalculatedColumn ID="calcHigher" Formula="@Data.MetricTarget~ * 1.20" />
                                      <CalculatedColumn ID="calcValue" Formula="@Data.MetricValue~" />
                                      <CalculatedColumn ID="calcTarget" Formula="@Data.MetricTarget~" />
                                    </DataLayer>
                                    <GaugeRing RingEndValue="@Data.calcHigher~" RingStartValue="@Data.calcTarget~" Color="#5EC45E" RingThickness="20" />
                                    <GaugeRing RingEndValue="@Data.calcTarget~" RingStartValue="@Data.calcLower~" RingThickness="20" Color="red" />
                                    <GaugeLabel Caption="Actual" FontSize="8" Top="132" FontBold="True" />
                                    <GaugeLabel Caption="Target" FontSize="8" Top="-12" FontBold="True" />
                                    <GaugeLabel Caption="@Data.calcValue~" FontSize="24" Top="145" Format="$#,##0" />
                                  </Gauge>
                                  <LineBreak />
                                </Column>
                              </Row>
                            </Rows>
                          </Column>
                        </Row>
                      </Rows>
                    </Column>
                    <Column Class="gdMetricContainer,ThemeAlignTop">
                      <Rows>
                        <Row>
                          <Column>
                            <Rows Class="gdMetricContainer">
                              <Row>
                                <Column Class="ThemeAlignCenter,gdMetricContainerHeader">
                                  <Label Caption="Conversion Rate" />
                                </Column>
                              </Row>
                              <Row>
                                <Column Class="ThemeAlignTop,ThemeAlignCenter,gdMetricContainerContent">
                                  <Gauge Type="Needle" ChartHeight="222" ID="gaugeConvRateMonth" Value="@Data.calcValue~" ChartWidth="263" StartAngle="0" UpperBound="@Data.calcHigher~" TickCount="2" NeedleGaugeEnclosure="None" D3Effects="True" BackgroundColor="#DDEBF7" NeedleThickness="1" GaugeType="HalfTop" LowerBound="@Data.calcLower~" TickLabelFontSize="8" TickLabelRadius="110" MinorTickFrequency="5" TickLabelFormat="@Data.MetricFormat~" CenterOffsetY="-8">
                                    <DataLayer Type="Linked" LinkedDataLayerID="rptMetricsMonth" ID="rptMetricsMonth.Linked3">
                                      <ConditionFilter Condition="&quot;@Data.MetricName~&quot; = &quot;Conv Rate&quot;" IncludeCondition="True" ID="cfConvRate" />
