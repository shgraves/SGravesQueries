  <LocalData ID="RepData">
    <DataLayer Type="SQL" Source="USE CallCenterReporting&#xD;&#xA;&#xD;&#xA;SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED&#xD;&#xA;&#xD;&#xA;SELECT		rh.Manager_UserID AS MgrUserID&#xD;&#xA;			,CASE WHEN mu.isCallCenter = 1 AND (D.isInbound = 1 OR D.isOutbound = 1 OR D.isTraining = 1) THEN D.DepartmentID ELSE 99 END AS DepartmentID&#xD;&#xA;			,CASE WHEN rh.RepType = 1 AND mu.isCallCenter = 1 AND (D.isInbound = 1 OR D.isOutbound = 1 OR D.isTraining = 1) THEN rh.SupervisorID&#xD;&#xA;				  WHEN rh.RepType = 3 AND mu.isCallCenter = 1 AND (D.isInbound = 1 OR D.isOutbound = 1 OR D.isTraining = 1) THEN rh.Manager_UserID&#xD;&#xA;				  ELSE -1&#xD;&#xA;			 END AS SupID&#xD;&#xA;			,CASE WHEN rh.RepType = 1 THEN 1 &#xD;&#xA;				  WHEN rh.RepType IN (3,5) THEN 3 &#xD;&#xA;				  ELSE 9 END AS RepType&#xD;&#xA;FROM		dbo.manager_users mu&#xD;&#xA;JOIN		dbo.crm_RepHierarchy rh ON mu.id = rh.Manager_UserID&#xD;&#xA;									AND rh.isActive = 1&#xD;&#xA;LEFT JOIN	dbo.fn_ActiveC3Departments() D ON D.DepartmentID = rh.Department&#xD;&#xA;WHERE		mu.login_name = '@Request.username~'" ID="dlRepID" ConnectionID="connP3BIG-CallCenterReporting" />
  </LocalData>
