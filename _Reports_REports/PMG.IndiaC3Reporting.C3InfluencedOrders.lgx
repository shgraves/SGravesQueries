      <DataLayer Type="SQL" Source="SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @StartDate datetime = '@Request.rptStartDate~',&#xD;&#xA;		@EndDate datetime = '@Request.rptEndDate~'&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @IndianOffSetMinutes Int SELECT @IndianOffSetMinutes = (12 * 60 + 30)&#xD;&#xA;&#xD;&#xA;SET @StartDate = DATEADD(MINUTE, -@IndianOffSetMinutes, @StartDate)&#xD;&#xA;SET @EndDate = DATEADD(MINUTE, -@IndianOffSetMinutes, @EndDate)&#xD;&#xA;&#xD;&#xA;if OBJECT_ID('tempdb.dbo.#orders') is not null drop table #orders&#xD;&#xA;select&#xD;&#xA;    o.orderid,&#xD;&#xA;    o.OrderDate,&#xD;&#xA;    DATEADD(MINUTE, @IndianOffSetMinutes, OrderDate) as india_orderdate,&#xD;&#xA;    crm_AccessDate,&#xD;&#xA;    o.RepVersion,&#xD;&#xA;    mu.lastname + ', ' + mu.firstname + ' ' + mu.middlename as agentName,&#xD;&#xA;    PointOfPurchaseId,&#xD;&#xA;    OrderTotal*0.01 as GCR,&#xD;&#xA;case when bill_to_countryN = 'in' then 'India'&#xD;&#xA; when bill_to_countryN = 'cn' then 'China'&#xD;&#xA; else 'Other' end as billing_country&#xD;&#xA;into #orders&#xD;&#xA;from [goDaddyCalls].[dbo].[rptGenPactOrders] o (nolock)&#xD;&#xA;join GoDaddy.dbo.gdshop_receipt_header h (nolock) on o.orderid = h.order_id&#xD;&#xA;join GDReportStaging.dbo.crm_RepHierarchy  rh WITH (NOLOCK) on o.RepVersion = rh.RepVersion&#xD;&#xA;join GDReportStaging.dbo.manager_users mu WITH (NOLOCK) ON rh.Manager_UserID = mu.ID and mu.locationID = 39&#xD;&#xA;where&#xD;&#xA;not exists -- ignore web orders from EA customers&#xD;&#xA;    (&#xD;&#xA;    select 1 from GoDaddyCalls.dbo.rptAccountMgmtAudit a (nolock)&#xD;&#xA;        where h.shopper_id = a.shopper_id and crm_portfoliotypeid in ( 1, 6, 7 ) -- ea&#xD;&#xA;        and PointOfPurchaseId = 1&#xD;&#xA;        and a.startdate &lt;= h.date_entered and h.date_entered &lt; a.enddate&#xD;&#xA;    )&#xD;&#xA;and date_entered &gt;= @StartDate and date_entered &lt; @EndDate&#xD;&#xA;and orderdate &gt;= @StartDate and orderdate &lt; dateadd(day, 2, @EndDate);&#xD;&#xA;&#xD;&#xA;create unique clustered index x_order on #orders(orderid)&#xD;&#xA;&#xD;&#xA;alter table #orders add new_gcr float, renew_gcr float, is_first_order bit&#xD;&#xA;&#xD;&#xA;update o&#xD;&#xA;set new_gcr = x.new_gcr,&#xD;&#xA;    renew_gcr = x.renew_gcr,&#xD;&#xA;    is_first_order = x.is_first_order&#xD;&#xA;from #orders o&#xD;&#xA;join&#xD;&#xA;    (&#xD;&#xA;    select&#xD;&#xA;        orderid,&#xD;&#xA;        od.is_first_order,&#xD;&#xA;        SUM(case when New_Renewal = 'new purchase' then adjusted_price*0.01 else 0 end) as new_gcr,&#xD;&#xA;        SUM(case when New_Renewal = 'renewal' then adjusted_price*0.01 else 0 end) as renew_gcr&#xD;&#xA;    from #orders o&#xD;&#xA;    join BIGReporting.dbo.rptOrderDetail od (nolock) on o.orderid = od.order_id and od.order_date &gt;= @StartDate and od.order_date &lt; dateadd(day, 2, @EndDate)&#xD;&#xA;    join DMStaging.dbo.dim_product dp (nolock) on od.pf_id = dp.pf_id&#xD;&#xA;    group by orderid, od.is_first_order&#xD;&#xA;    ) x on o.orderid = x.orderid&#xD;&#xA;option(maxdop 1);&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;select&#xD;&#xA;    case when od.point_of_purchase_id = 1 then 'FOS' when od.point_of_purchase_id = 3 then 'C3' end as POP&#xD;&#xA;            , CASE                                                                                              &#xD;&#xA;                WHEN COALESCE(sc.source_code, '') = '' THEN 'Branding'                                             &#xD;&#xA;                WHEN od.channel_ID = 20 THEN 'Advocacy'                                                            &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID =  4 THEN 'Affiliates'                               &#xD;&#xA;                WHEN od.channel_ID = 17 THEN 'Affiliates'                                                          &#xD;&#xA;                WHEN od.channel_ID = 16 THEN 'Branding'                                                            &#xD;&#xA;                WHEN od.channel_ID = 10 THEN 'Branding'                                                            &#xD;&#xA;                WHEN od.channel_ID =  5 THEN 'Branding'                                                            &#xD;&#xA;                WHEN od.channel_ID =  3 THEN 'eMail'                                                               &#xD;&#xA;                WHEN od.channel_ID =  4 THEN 'eMail'                                                               &#xD;&#xA;                WHEN od.channel_ID = 11 THEN 'Parked Pages'                                                        &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID IN (2) THEN 'Podcasting'                             &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID IN (13,85) THEN 'Online'                             &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID = 86 THEN 'Social Media'                             &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID in (87,88,89,90) THEN 'Reseller and Partnerships'    &#xD;&#xA;                WHEN od.channel_ID = 12 THEN 'Reseller and Partnerships'                                           &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID =  1 THEN 'Search'                                   &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID = 83 THEN 'Search'                                   &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID = 10 THEN 'Viral'                                    &#xD;&#xA;                ELSE 'Other'                                                                                       &#xD;&#xA;                END as Channel&#xD;&#xA;    , count(distinct case when od.is_first_order = 1 then o.orderid end) as new_custs&#xD;&#xA;    , count(distinct order_id) as orders&#xD;&#xA;    , sum(adjusted_price*0.01) as gcr&#xD;&#xA;    , sum(case when new_renewal = 'new purchase' then adjusted_price*0.01 else 0 end) as new_gcr&#xD;&#xA;    , sum(case when new_renewal = 'renewal' then adjusted_price*0.01 else 0 end) as renew_gcr&#xD;&#xA;from #orders o&#xD;&#xA;join bigreporting.dbo.rptOrderDetail od (nolock) on o.orderid = od.order_id&#xD;&#xA;join DMStaging.dbo.dim_product dp (nolock) on od.pf_id = dp.pf_id&#xD;&#xA;left join godaddy.dbo.isc_sourcecode sc (nolock) on od.source_code = sc.source_code&#xD;&#xA;where od.order_date &gt;= @StartDate and od.order_date &lt; dateadd(day, 2, @EndDate)&#xD;&#xA;group by &#xD;&#xA;    case when od.point_of_purchase_id = 1 then 'FOS' when od.point_of_purchase_id = 3 then 'C3' end&#xD;&#xA;            , CASE                                                                                              &#xD;&#xA;                WHEN COALESCE(sc.source_code, '') = '' THEN 'Branding'                                             &#xD;&#xA;                WHEN od.channel_ID = 20 THEN 'Advocacy'                                                            &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID =  4 THEN 'Affiliates'                               &#xD;&#xA;                WHEN od.channel_ID = 17 THEN 'Affiliates'                                                          &#xD;&#xA;                WHEN od.channel_ID = 16 THEN 'Branding'                                                            &#xD;&#xA;                WHEN od.channel_ID = 10 THEN 'Branding'                                                            &#xD;&#xA;                WHEN od.channel_ID =  5 THEN 'Branding'                                                            &#xD;&#xA;                WHEN od.channel_ID =  3 THEN 'eMail'                                                               &#xD;&#xA;                WHEN od.channel_ID =  4 THEN 'eMail'                                                               &#xD;&#xA;                WHEN od.channel_ID = 11 THEN 'Parked Pages'                                                        &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID IN (2) THEN 'Podcasting'                             &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID IN (13,85) THEN 'Online'                             &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID = 86 THEN 'Social Media'                             &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID in (87,88,89,90) THEN 'Reseller and Partnerships'    &#xD;&#xA;                WHEN od.channel_ID = 12 THEN 'Reseller and Partnerships'                                           &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID =  1 THEN 'Search'                                   &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID = 83 THEN 'Search'                                   &#xD;&#xA;                WHEN od.channel_ID =  1 AND od.sub_channel_ID = 10 THEN 'Viral'                                    &#xD;&#xA;                ELSE 'Other'                                                                                       &#xD;&#xA;                END &#xD;&#xA;order by pop, gcr desc&#xD;&#xA;    ;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" ID="dlIndiaInfluencedOrders" ConnectionID="connM1BIG-biLogiReporting" />
    </DataLayer>
  </LocalData>
