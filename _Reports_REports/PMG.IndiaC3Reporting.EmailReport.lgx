      <Row ID="rowOne">
        <Column ID="colContactsByDate" Class="gdColumnCell">
          <Division ID="divContactsByDate">
            <DataTable ID="dtContactsByDate" CellSpacing="5 px" AltRowClass="gdThemeAltRowNoPadding td" ColumnHeaderClass="gdTableHeader" CaptionClass="gdColumnCellTitle">
