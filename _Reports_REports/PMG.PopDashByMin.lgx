        <Column ID="colC3Calls" Class="gdDefaultDivision">
          <Division Class="ThemeAlignCenter">
            <Rows>
              <Row>
                <Column Class="gdColumnCell">
                  <Chart Type="XY" XYChartType="Line" ChartDataColumn="Current_Calls" ChartHeight="260" ChartWidth="400" ChartTitle="C3 Inbound Calls – by Half Hour" ChartLabelColumn="Date" ChartLabelColumnDataType="Date" BackgroundColor="White" Color="#E55CDB" ID="chtC3CallsMin" GridVerticalColor="#f2f2f2" GridHorizontalColor="#f2f2f2" BorderTop="30" LineStyle="SolidLine" ChartSymbolEdgeColor="Black" BorderLeft="55" BorderRight="10" BorderBottom="70" LineWidth="2" LegendLabel="Calls" BorderColor="transparent">
                    <DataLayer Type="Cached" CacheKey="CallsByMin" ExpirationNow="False" ExpirationTimeSpan="00:01" ID="dlCallsbyMinCached">
                      <DataLayer Type="SQL" Source="USE BIGDashboards&#xD;&#xA;&#xD;&#xA;SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;&#xD;&#xA;&#xD;&#xA;DECLARE @ReportDate DATETIME&#xD;&#xA;DECLARE @EndDate DATETIME&#xD;&#xA;DECLARE @Days INT&#xD;&#xA;DECLARE @StartMinutes INT&#xD;&#xA;DECLARE @EndMinutes INT&#xD;&#xA;&#xD;&#xA;SELECT @EndDate = CONVERT(SMALLDATETIME,GETDATE())&#xD;&#xA;SELECT @ReportDate = CONVERT(DATE,GETDATE())&#xD;&#xA;SELECT @Days = -7&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @BaselineDates TABLE &#xD;&#xA;					(	BaselineKey INT IDENTITY&#xD;&#xA;						, Baseline DATETIME&#xD;&#xA;					)&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;WHILE (SELECT COUNT(*) FROM @BASELINEDATES) &lt; 5&#xD;&#xA;&#xD;&#xA;BEGIN&#xD;&#xA;	INSERT INTO @BASELINEDATES&#xD;&#xA;	SELECT dd.&quot;Date&quot; as Baseline&#xD;&#xA;	  FROM gdAnalyticDW.dbo.rptDimDate  dd&#xD;&#xA;	  WHERE Date = DATEADD(DAY,@Days,CONVERT(DATE,@ReportDate))&#xD;&#xA;	  AND HolidayFlag &lt;&gt; 1&#xD;&#xA;SET @Days = @Days -7&#xD;&#xA;END&#xD;&#xA;&#xD;&#xA;SELECT @StartMinutes = (DATEPART(HOUR, @ReportDate) * 60) +&#xD;&#xA;					   (DATEPART(MINUTE, @ReportDate))&#xD;&#xA;&#xD;&#xA;SELECT @EndMinutes   = (DATEPART(HOUR, @EndDate) * 60) +&#xD;&#xA;					   (DATEPART(MINUTE, @EndDate))&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @DateRanges TABLE (&#xD;&#xA;							StartDate DATETIME&#xD;&#xA;							, EndDate DATETIME&#xD;&#xA;							, WeekType VARCHAR(10)&#xD;&#xA;						   )&#xD;&#xA;&#xD;&#xA;INSERT INTO @DateRanges&#xD;&#xA;SELECT @ReportDate&#xD;&#xA;		, @EndDate&#xD;&#xA;		, 'Current'&#xD;&#xA;UNION&#xD;&#xA;SELECT DATEADD(MINUTE,@StartMinutes,Baseline)&#xD;&#xA;		, DATEADD(MINUTE,@EndMinutes,Baseline)&#xD;&#xA;		, 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 1&#xD;&#xA;UNION&#xD;&#xA;SELECT DATEADD(MINUTE,@StartMinutes,Baseline)&#xD;&#xA;		, DATEADD(MINUTE,@EndMinutes,Baseline)&#xD;&#xA;		, 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 2&#xD;&#xA;UNION&#xD;&#xA;SELECT DATEADD(MINUTE,@StartMinutes,Baseline)&#xD;&#xA;		, DATEADD(MINUTE,@EndMinutes,Baseline)&#xD;&#xA;		, 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 3&#xD;&#xA;&#xD;&#xA;SELECT CAST(CONVERT(TIME,CM.dateInterval) AS DATETIME) AS &quot;Date&quot;&#xD;&#xA;	,COALESCE(SUM(CASE WHEN DR.weektype = 'Current' THEN CM.nco END),1.7e308) AS Current_Calls&#xD;&#xA;	,SUM(CASE WHEN DR.weektype = 'Baseline' THEN CM.nco END) / 3 AS Baseline_Calls&#xD;&#xA;FROM dbo.iDashboardCallsMart CM (NOLOCK)&#xD;&#xA;JOIN @DateRanges DR&#xD;&#xA;	ON CM.dateInterval &gt;= DR.StartDate&#xD;&#xA;	AND CM.dateInterval &lt; DR.EndDate&#xD;&#xA;WHERE isReady = 1&#xD;&#xA;GROUP BY CAST(CONVERT(TIME,CM.dateInterval) AS DATETIME) " ConnectionID="connM1BIG-biLogiReporting" ID="dlCallsbyMin">
                        <SortFilter SortColumn="Date" SortSequence="Ascending" />
                      </DataLayer>
                    </DataLayer>
                    <ExtraXYDataColumn ChartDataColumn="Baseline_Calls" Color="#010101" LegendLabel="Baseline" LineWidth="2" />
                    <FontChartTitle FontFilename="Arial" FontColor="black" FontSize="14" Alignment="Center" />
                    <LabelScale Format="HH:mm" ScalingMode="LinearTime" />
                    <FontXAxisTitle FontFilename="arial" FontColor="Black" FontSize="8" />
