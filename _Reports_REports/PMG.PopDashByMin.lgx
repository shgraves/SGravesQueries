  <LocalData>
    <DataLayer Type="SQL" Source="SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;&#xD;&#xA;SET NOCOUNT ON;&#xD;&#xA;&#xD;&#xA;DECLARE @ReportDate DATETIME&#xD;&#xA;DECLARE @EndDate DATETIME&#xD;&#xA;DECLARE @Days INT&#xD;&#xA;DECLARE @EndMinutes INT&#xD;&#xA;&#xD;&#xA;SELECT @EndDate = DATEADD(MINUTE,-1,CONVERT(SMALLDATETIME,GETDATE()))&#xD;&#xA;SELECT @ReportDate = CONVERT(DATE,GETDATE())&#xD;&#xA;SELECT @Days = -7&#xD;&#xA;&#xD;&#xA;DECLARE @Msg varchar(255)&#xD;&#xA;&#xD;&#xA;DECLARE @BaselineDates TABLE &#xD;&#xA;                              (     BaselineKey INT IDENTITY&#xD;&#xA;                                    , Baseline DATETIME&#xD;&#xA;                              )&#xD;&#xA;&#xD;&#xA;       SET @Msg = '  Reading from GDAnalyticDW.dbo.rptDimDate ' + CONVERT(VARCHAR(100), getdate(), 114)&#xD;&#xA;       RAISERROR(@Msg, 0, 1) WITH NOWAIT&#xD;&#xA;&#xD;&#xA;WHILE (SELECT COUNT(*) FROM @BASELINEDATES) &lt; 5&#xD;&#xA;&#xD;&#xA;BEGIN&#xD;&#xA;      INSERT INTO @BASELINEDATES&#xD;&#xA;      SELECT dd.&quot;Date&quot; as Baseline&#xD;&#xA;        FROM GDAnalyticDW.dbo.rptDimDate dd&#xD;&#xA;        WHERE Date = DATEADD(DAY,@Days,CONVERT(DATE,@ReportDate))&#xD;&#xA;        AND HolidayFlag &lt;&gt; 1&#xD;&#xA;SET @Days = @Days -7&#xD;&#xA;END&#xD;&#xA;&#xD;&#xA;SELECT @EndMinutes   = (DATEPART(HOUR, @EndDate) * 60) +&#xD;&#xA;                                 (DATEPART(MINUTE, @EndDate))&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @DateRanges TABLE (&#xD;&#xA;                                          StartDate DATETIME PRIMARY KEY CLUSTERED NOT NULL&#xD;&#xA;                                          , EndDate DATETIME&#xD;&#xA;                                          , WeekType VARCHAR(10)&#xD;&#xA;                                      )&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;       SET @Msg = '  Populating date ranges and baseline dates ' + CONVERT(VARCHAR(100), getdate(), 114)&#xD;&#xA;       RAISERROR(@Msg, 0, 1) WITH NOWAIT&#xD;&#xA;&#xD;&#xA;INSERT INTO @DateRanges&#xD;&#xA;SELECT @ReportDate&#xD;&#xA;            , @EndDate&#xD;&#xA;            , 'Current'&#xD;&#xA;UNION&#xD;&#xA;SELECT Baseline&#xD;&#xA;            , DATEADD(MINUTE,@EndMinutes,Baseline)&#xD;&#xA;            , 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 1&#xD;&#xA;UNION&#xD;&#xA;SELECT Baseline&#xD;&#xA;            , DATEADD(MINUTE,@EndMinutes,Baseline)&#xD;&#xA;            , 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 2&#xD;&#xA;UNION&#xD;&#xA;SELECT Baseline&#xD;&#xA;            , DATEADD(MINUTE,@EndMinutes,Baseline)&#xD;&#xA;            , 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 3&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;       SET @Msg = '  Select from the VIEW ' + CONVERT(VARCHAR(100), getdate(), 114)&#xD;&#xA;       RAISERROR(@Msg, 0, 1) WITH NOWAIT&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;SELECT * INTO #currentOrders FROM BIGReporting.dbo.rptOrderDetailLive_vw r  &#xD;&#xA;&#xD;&#xA;       SET @Msg = '  Select from the cte ' + CONVERT(VARCHAR(100), getdate(), 114)&#xD;&#xA;       RAISERROR(@Msg, 0, 1) WITH NOWAIT&#xD;&#xA;;&#xD;&#xA;WITH myordersCTE AS (&#xD;&#xA;SELECT  CAST(CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1)) AS DATETIME) AS &quot;DATE&quot;&#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'current' AND point_of_purchase_id = 1 THEN order_id END) AS current_web_orders &#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 1 THEN order_id END) / 3 AS baseline_web_orders &#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'current' AND point_of_purchase_id = 3 THEN order_id END) AS current_c3_orders &#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 3 THEN order_id END) / 3 AS baseline_c3_orders &#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'current' AND point_of_purchase_id = 1 THEN adjusted_price * .01 END),0) AS current_web_gcr&#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 1 THEN adjusted_price * .01 END) / 3,0) AS baseline_web_gcr&#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'current' AND point_of_purchase_id = 3 THEN adjusted_price * .01 END),0) AS current_c3_gcr&#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 3 THEN adjusted_price * .01 END) / 3,0) AS baseline_c3_gcr&#xD;&#xA;, CASE WHEN CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1))  &gt;= DATEADD(mi,-60,CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,GETDATE())),-1)) ) THEN 1 ELSE 0 END AS zoom_flag&#xD;&#xA;FROM BIGReporting.dbo.rptOrderDetail r WITH (NOLOCK)&#xD;&#xA;JOIN @DateRanges b  ON r.order_date &gt;= b.startdate AND  r.order_date &lt; b.enddate&#xD;&#xA;WHERE point_of_purchase_id IN (1,3)&#xD;&#xA;GROUP BY  CAST(CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1)) AS DATETIME)&#xD;&#xA;, CASE WHEN CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1))  &gt;= DATEADD(mi,-60,CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,GETDATE())),-1)) ) THEN 1 ELSE 0 END&#xD;&#xA;UNION ALL&#xD;&#xA;SELECT  CAST(CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1)) AS DATETIME) AS &quot;DATE&quot;&#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'current' AND point_of_purchase_id = 1 THEN order_id END) AS current_web_orders &#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 1 THEN order_id END) / 3 AS baseline_web_orders &#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'current' AND point_of_purchase_id = 3 THEN order_id END) AS current_c3_orders &#xD;&#xA;, COUNT (DISTINCT CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 3 THEN order_id END) / 3 AS baseline_c3_orders &#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'current' AND point_of_purchase_id = 1 THEN adjusted_price * .01 END),0) AS current_web_gcr&#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 1 THEN adjusted_price * .01 END) / 3,0) AS baseline_web_gcr&#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'current' AND point_of_purchase_id = 3 THEN adjusted_price * .01 END),0) AS current_c3_gcr&#xD;&#xA;, ISNULL(SUM (CASE WHEN weektype = 'baseline' AND point_of_purchase_id = 3 THEN adjusted_price * .01 END) / 3,0) AS baseline_c3_gcr&#xD;&#xA;, CASE WHEN CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1))  &gt;= DATEADD(mi,-60,CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,GETDATE())),-1)) ) THEN 1 ELSE 0 END AS zoom_flag&#xD;&#xA;FROM #currentOrders r&#xD;&#xA;JOIN @DateRanges b  ON r.order_date &gt;= b.startdate AND  r.order_date &lt; b.enddate&#xD;&#xA;WHERE point_of_purchase_id IN (1,3)&#xD;&#xA;AND EXISTS (SELECT MAX(ORDER_DATE) newestRecord, GETDATE() currentDate FROM  bigreporting.dbo.rptOrderDetail WITH (NOLOCK))&#xD;&#xA;GROUP BY CAST(CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1)) AS DATETIME)&#xD;&#xA;, CASE WHEN CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,order_date)),-1))  &gt;= DATEADD(mi,-60,CONVERT(TIME, DATEADD(MI,(DATEDIFF(mi,-1,GETDATE())),-1)) ) THEN 1 ELSE 0 END&#xD;&#xA;)&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;SELECT &quot;Date&quot;,&#xD;&#xA;            SUM(current_web_orders) AS current_web_orders,&#xD;&#xA;            SUM(baseline_web_orders) AS baseline_web_orders,&#xD;&#xA;            SUM(current_c3_orders) AS current_c3_orders,&#xD;&#xA;            SUM(baseline_c3_orders) AS baseline_c3_orders,&#xD;&#xA;            SUM(current_web_gcr) AS current_web_gcr,&#xD;&#xA;            SUM(baseline_web_gcr) AS baseline_web_gcr,&#xD;&#xA;            SUM(current_c3_gcr) AS current_c3_gcr,&#xD;&#xA;            SUM(baseline_c3_gcr) AS baseline_c3_gcr,&#xD;&#xA;            MAX(zoom_flag) AS zoom_flag&#xD;&#xA;FROM myordersCTE&#xD;&#xA;GROUP BY &quot;Date&quot;&#xD;&#xA;&#xD;&#xA;drop table #currentOrders&#xD;&#xA;" ConnectionID="connM1BIG-biLogiReporting" ID="dlPOPsalesbyMin" IdeDisplayStatus="Collapsed">
