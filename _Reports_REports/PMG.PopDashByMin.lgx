                    <DataLayer Type="Cached" CacheKey="TrafficbyMin" ExpirationNow="False" ExpirationTimeSpan="00:01" ID="dlTrafficbyMinCached">
                      <DataLayer Type="SQL" Source="USE BIGDashboards &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;&#xD;&#xA;&#xD;&#xA;DECLARE @ReportDate DATETIME&#xD;&#xA;DECLARE @EndDate DATETIME&#xD;&#xA;DECLARE @Days INT&#xD;&#xA;DECLARE @StartSeconds INT&#xD;&#xA;DECLARE @EndSeconds INT&#xD;&#xA;&#xD;&#xA;SELECT @EndDate = CONVERT(SMALLDATETIME,GETDATE())&#xD;&#xA;SELECT @ReportDate = CONVERT(DATE,GETDATE())&#xD;&#xA;SELECT @Days = -7&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @BaselineDates TABLE &#xD;&#xA;					(	BaselineKey INT IDENTITY&#xD;&#xA;						, Baseline DATETIME&#xD;&#xA;					)&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;WHILE (SELECT COUNT(*) FROM @BASELINEDATES) &lt; 5&#xD;&#xA;&#xD;&#xA;BEGIN&#xD;&#xA;	INSERT INTO @BASELINEDATES&#xD;&#xA;	SELECT dd.&quot;Date&quot; as Baseline&#xD;&#xA;	  FROM gdAnalyticDW.dbo.rptDimDate dd&#xD;&#xA;	  WHERE Date = DATEADD(DAY,@Days,CONVERT(DATE,@ReportDate))&#xD;&#xA;	  AND HolidayFlag &lt;&gt; 1&#xD;&#xA;SET @Days = @Days -7&#xD;&#xA;END&#xD;&#xA;&#xD;&#xA;SELECT @StartSeconds = (DATEPART(HOUR, @ReportDate) * 3600) +&#xD;&#xA;					   (DATEPART(MINUTE, @ReportDate) * 60) +&#xD;&#xA;						DATEPART(SECOND, @ReportDate)&#xD;&#xA;&#xD;&#xA;SELECT @EndSeconds   = (DATEPART(HOUR, @EndDate) * 3600) +&#xD;&#xA;					   (DATEPART(MINUTE, @EndDate) * 60) +&#xD;&#xA;						DATEPART(SECOND, @EndDate)&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;DECLARE @DateRanges TABLE (&#xD;&#xA;							StartDate DATETIME&#xD;&#xA;							, EndDate DATETIME&#xD;&#xA;							, WeekType VARCHAR(10)&#xD;&#xA;						   )&#xD;&#xA;&#xD;&#xA;INSERT INTO @DateRanges&#xD;&#xA;SELECT @ReportDate&#xD;&#xA;		, @EndDate&#xD;&#xA;		, 'Current'&#xD;&#xA;UNION&#xD;&#xA;SELECT DATEADD(SECOND,@StartSeconds,Baseline)&#xD;&#xA;		, DATEADD(SECOND,@EndSeconds,Baseline)&#xD;&#xA;		, 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 1&#xD;&#xA;UNION&#xD;&#xA;SELECT DATEADD(SECOND,@StartSeconds,Baseline)&#xD;&#xA;		, DATEADD(SECOND,@EndSeconds,Baseline)&#xD;&#xA;		, 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 2&#xD;&#xA;UNION&#xD;&#xA;SELECT DATEADD(SECOND,@StartSeconds,Baseline)&#xD;&#xA;		, DATEADD(SECOND,@EndSeconds,Baseline)&#xD;&#xA;		, 'Baseline'&#xD;&#xA;FROM @BASELINEDATES &#xD;&#xA;WHERE BaselineKey = 3&#xD;&#xA;&#xD;&#xA;SELECT CAST(CONVERT(TIME,TM.dateInterval) AS DATETIME) AS &quot;Date&quot;&#xD;&#xA;	,COALESCE(SUM(CASE WHEN DR.weektype = 'Current' THEN TM.visits END),1.7e308) AS Current_Traffic&#xD;&#xA;	,SUM(CASE WHEN DR.weektype = 'Baseline' THEN TM.visits END) / 3 AS Baseline_Traffic&#xD;&#xA;FROM dbo.iDashboardTrafficMart TM (NOLOCK)&#xD;&#xA;JOIN @DateRanges DR&#xD;&#xA;	ON TM.dateInterval &gt;= DR.StartDate&#xD;&#xA;	AND TM.dateInterval &lt; DR.EndDate&#xD;&#xA;WHERE isReady = 1&#xD;&#xA;	AND siteID = 1&#xD;&#xA;GROUP BY CAST(CONVERT(TIME,TM.dateInterval) AS DATETIME) " ConnectionID="connM1BIG-biLogiReporting" ID="dlTrafficbyMin">
