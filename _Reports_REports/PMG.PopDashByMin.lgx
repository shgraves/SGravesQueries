                      <DataLayer Type="SQL" ConnectionID="connM1BIG-BIGReporting" Source="--Country&#xD;&#xA;select case when region_name = 'North America' then '04'&#xD;&#xA;			when region_name = 'South America' then '05'&#xD;&#xA;			when region_name = 'Asia' then '01'&#xD;&#xA;			when region_name in ('Caribbean','Central America') then '06'&#xD;&#xA;			when region_name = 'Europe' then '02'&#xD;&#xA;			when region_name = 'Africa' then '03'&#xD;&#xA;			when region_name = 'Middle East' then '08'&#xD;&#xA;			when region_name = 'Australia and Oceania' then '07' else region_name end as region_name &#xD;&#xA;, sum (adjusted_price * .01) as gcr&#xD;&#xA;FROM bigreporting.dbo.rptorderdetail r (nolock)&#xD;&#xA;JOIN bigreporting.dbo.rptcountry c (nolock) on r.bill_to_country = c.country_code&#xD;&#xA;JOIN bigreporting.dbo.rptregion rg (nolock) on rg.region_id = c.region_id&#xD;&#xA;where order_date &gt;= dateadd(hh,-1,getdate())&#xD;&#xA;and order_date &lt; getdate()&#xD;&#xA;group by case when region_name = 'North America' then '04'&#xD;&#xA;			when region_name = 'South America' then '05'&#xD;&#xA;			when region_name = 'Asia' then '01'&#xD;&#xA;			when region_name in ('Caribbean','Central America') then '06'&#xD;&#xA;			when region_name = 'Europe' then '02'&#xD;&#xA;			when region_name = 'Africa' then '03'&#xD;&#xA;			when region_name = 'Middle East' then '08'&#xD;&#xA;			when region_name = 'Australia and Oceania' then '07' else region_name end&#xD;&#xA;order by 2 desc&#xD;&#xA;" ID="dlMap">
