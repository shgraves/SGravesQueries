            <XolapCube ID="XolapCube1">
              <DataLayer Type="SQL" ID="SQLDataLayer" ConnectionID="connM1BIG-BIGReporting" Source="SELECT			'2013-11-01' AS order_date, c.name				AS channel_id&#xD;&#xA;				, r.source_code		AS source_code&#xD;&#xA;				, r.adjusted_price * 0.01	AS gcr&#xD;&#xA;				, r.order_id			as order_id&#xD;&#xA;FROM			bigreporting.dbo.rptorderdetail r (NOLOCK)&#xD;&#xA;JOIN			godaddy.dbo.isc_channel c (NOLOCK)&#xD;&#xA;ON				r.channel_Id = c.channel_id&#xD;&#xA;AND				r.order_date &gt;= '2013-11-01'&#xD;&#xA;AND				r.order_date &lt;  '2013-11-02'&#xD;&#xA;AND				(r.source_code LIKE '%1964%' OR r.source_code IN ('???', '?SR'))&#xD;&#xA;">
                <TimePeriodColumn DataColumn="order_date" TimePeriod="Date" ID="tpcOrderDate" />
              </DataLayer>
              <XolapDimensions>
                <XolapDimension ID="dimsource_code" DimensionName="[source_code]">
                  <XolapLevel ID="lvlsourcecode" DataColumn="source_code" LevelName="source code" />
                  <XolapLevel ID="lvlchannelid" DataColumn="channel_id" LevelName="channel id" />
                </XolapDimension>
              </XolapDimensions>
              <XolapMeasures>
                <XolapMeasure ID="measGCR" MeasureName="GCR" DataColumn="gcr" Function="Sum" />
              </XolapMeasures>
            </XolapCube>
          </Remark>
          <DimensionGridTablePanel />
        </DimensionGrid>
